# Gemini CLI Docker System Environment Configuration
# Copy this file to .env and fill in your actual values

# Gemini API Key (required)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Main Service Configuration
MAIN_SERVICE_PORT=8081
MAIN_SERVICE_HOST=0.0.0.0

# Docker Configuration
DOCKER_IMAGE_NAME=agent-service:latest
AGENT_SERVICE_PORT=5000

# Git Configuration (for containers)
GIT_USER_NAME="AI Developer"
GIT_USER_EMAIL="ai@example.com"
GIT_DEFAULT_BRANCH=main

# Demo Configuration
DEMO_REPO_URL=https://github.com/octocat/Hello-World.git
